{
  "plugins": ["prettier-plugin-astro"],
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "bracketSpacing": true,
  "objectWrap": "collapse",
  "bracketSameLine": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "overrides": [{ "files": "*.astro", "options": { "parser": "astro" } }]
}
